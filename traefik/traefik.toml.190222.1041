#zf 190222.1036

# Service logs (here debug mode)
debug = true
traefikLogsFile = "/var/log/traefik/traefik.log"
logLevel = "DEBUG"

# Entrypoints
[entryPoints]
    [entryPoints.http]
    address = ":80"


[entryPoints.http.redirect]
    regex = "^http://guide.museebolo.cf/(.*)"
    replacement = "http://z.zufferey.com/bolo/bolo/new_bolo/home.html"


#        [entryPoints.http.redirect]
#        entryPoint = "https"
#    [entryPoints.https]
#    address = ":443"
#        [entryPoints.https.tls]
        
# Access log
filePath = "/var/log/traefik/access.log"
format = "common"

# Admin dashboard
[web]
address = ":8080"

[file]
filename = "/etc/traefik/traefik.toml"
watch = true


# rules
[backends]
    [backends.backend1]
        [backends.backend1.servers]
            [backends.backend1.servers.server0]
                url = "http://51.158.76.194:8001"

    [backends.backend2]
        [backends.backend2.servers]
            [backends.backend2.servers.server0]
                url = "http://51.158.76.194:8002"

    [backends.backend3]
        [backends.backend3.servers]
            [backends.backend3.servers.server0]
                url = "http://z.zufferey.com/bolo/bolo/new_bolo/home.html"



[frontends]
    [frontends.frontend1]
    entrypoints = ["http", "https"]
    backend = "backend1"
    passHostHeader = true
    [frontends.frontend1.routes]
        [frontends.frontend1.routes.route0]
            rule = "Host:site1.zuzuwp.ml"

    [frontends.frontend2]
    entrypoints = ["http", "https"]
    backend = "backend2"
    passHostHeader = true
    [frontends.frontend2.routes]
        [frontends.frontend2.routes.route0]
            rule = "Host:site2.zuzuwp.ml"

    [frontends.frontend3]
    entrypoints = ["http", "https"]
    backend = "backend3"
    passHostHeader = true
    [frontends.frontend3.routes]
        [frontends.frontend3.routes.route0]
            rule = "Host:guide.museebolo.cf"

